---
layout: "layout-base"
tags: ["art"]
---

<h1>Art</h1>

{{ content | safe }}

<div class="art-container">
  {% for img in art | reverse %}
  <figure>
    <img
      src="{{ img.url }}"
      alt=""
      width="{{ img.width }}"
      height="{{ img.height }}"
      style="background-color: {{ img.color }};"
      loading="lazy"
    />
    <figcaption>
      <a href="{{ img.url }}">{{ img.name }}</a>
    </figcaption>
  </figure>
  {% endfor %}
</div>
